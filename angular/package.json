{
  "name": "mythaistar-restaurant",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -pc proxy.conf.json -o",
    "test": "ng test --browsers Chrome",
    "test:ci": "ng test --browsers ChromeHeadless --single-run",
    "test:firefox": "ng test --browsers Firefox",
    "test:ci:firefox": "ng test --browsers FirefoxHeadless --single-run",
    "test:firefox-dev": "ng test --browsers FirefoxDeveloper",
    "test:ci:firefox-dev": "ng test --browsers FirefoxDeveloperHeadless --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "serve": "ng serve --open --environment=dev",
    "serve:aot": "ng serve --open --environment=dev --aot",
    "serve:pwa": "npm run build:pwa && http-server dist -p 8080",
    "serve:prod": "ng serve --open --environment=prod",
    "serve:prod:aot": "ng serve --open --environment=prod --aot",
    "serve:prodcompose": "ng serve --open --environment=prodcompose",
    "serve:prodcompose:aot": "ng serve --open --environment=prodcompose --aot",
    "serve:node": "ng serve --open --environment=node",
    "serve:node:aot": "ng serve --open --environment=node --aot",
    "build": "ng build --environment=dev --aot",
    "build:pwa": "ng build --environment=pwa --prod --build-optimizer && npm run ngsw-config && npm run ngsw-copy",
    "build:prod": "ng build --environment=prod --prod --build-optimizer",
    "build:prodcompose": "ng build --environment=prodcompose --prod --build-optimizer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.6",
    "@angular/cdk": "~5.2.0",
    "@angular/common": "5.2.6",
    "@angular/compiler": "5.2.6",
    "@angular/core": "5.2.6",
    "@angular/forms": "5.2.6",
    "@angular/http": "5.2.6",
    "@angular/material": "~5.2.0",
    "@angular/platform-browser": "5.2.6",
    "@angular/platform-browser-dynamic": "5.2.6",
    "@angular/platform-server": "5.2.6",
    "@angular/router": "5.2.6",
    "@angular/service-worker": "5.2.6",
    "@covalent/core": "1.0.0",
    "@covalent/dynamic-forms": "1.0.0",
    "@covalent/highlight": "1.0.0",
    "@covalent/http": "1.0.0",
    "@covalent/markdown": "1.0.0",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "2.0.1",
    "angular2-moment": "^1.8.0",
    "core-js": "^2.5.3",
    "flag-icon-css": "^3.0.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.12.0",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "md2": "^0.0.29",
    "moment": "^2.20.1",
    "rxjs": "^5.5.6",
    "showdown": "^1.8.6",
    "whatwg-fetch": "^2.0.3",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "0.4.2",
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "5.2.6",
    "@angular/language-service": "5.2.6",
    "@types/hammerjs": "2.0.35",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "3.0.8",
    "codelyzer": "^4.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  },
  "optionalDependencies": {
    "@covalent/data": "^0.7.1"
  }
}
